<% include partials/header.ejs %>




<section id="index-content " class="container">
	<article class="row">
			<h1 class="titulo">Se cadastre</h1>
			<div class="col-sm-12">
				<form action="/user" method="POST" class="card">
				  <div class="form-group">
				    <label for="name">Nome</label>
				    <input type="text" name="nome" class="form-control" id="nome" placeholder="Seu nome">
				  </div>

				 <div class="form-group">
				    <label for="exampleInputEmail1">Email</label>
				    <input type="email" name="email" class="form-control" id="email" placeholder="Email">
				  </div>

				 <div class="form-group">
				    <label for="exampleInputEmail1">idade</label>
				    <input type="number" name="idade" class="form-control" id="age" placeholder="Email" min="1" max="55">
				  </div>

				  <div class="form-group">
				    <label for="exampleInputPassword1">Password</label>
				    <input name="password" type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" >
				  </div>
				  <button type="submit" class="btn btn-default">Submit</button>
				</form>

			</div>

	</article>

	
</section>

